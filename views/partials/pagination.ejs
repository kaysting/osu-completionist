<div class="pages flex gap-4 justify-center">
    <% for (const num of pagination.nav) { %>
    <% if (num === '...') { %>
    <span class="dots flex align-center">...</span>
    <% } else { %>
    <btn onClick="reloadElement('#leaderboardContent', { url: '<%= pagination.basePath %>?p=<%= num %>', replaceAddress: true })"
        class="btn <%= num === pagination.current ? 'primary' : 'text' %>"><%= num %></btn>
    <% } %>
    <% } %>
</div>