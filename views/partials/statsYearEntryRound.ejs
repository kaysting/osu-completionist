<div class="yearlyStatsEntryRound rounded-4 flex gap-12 align-center">
    <%
        let percent = map_percentage_completed;
        let unit = 'maps';
        let completed = count_completed;
        let total = count_total;
        if (yearlyType == 'xp') {
            percent = time_percentage_completed;
            unit = 'cxp';
            completed = xp;
            total = xp_total;
        }
    %>
    <div class="speedometer" style="--percent: <%= percent %>; --color: <%= color %>;">
        <svg viewBox=" 0 0 100 100">
            <circle cx="50" cy="50" r="45" class="track" />
            <circle cx="50" cy="50" r="45" class="progress" />
        </svg>
        <div class="value"><%= Math.round(percent) %></div>
        <div class="unit">%</div>
    </div>
    <div class="flex col gap-2">
        <div class="year"><%= year %></div>
        <div class="progress">
            <%= completed.toLocaleString() %> / <%= total.toLocaleString() %> <%= unit %>
        </div>
    </div>
</div>