<div class="yearlyStatsEntry">
    <div class="year"><%= year %></div>
    <div class="content">
        <div class="values">
            <div class="percent">
                <%= (yearlyType == 'xp' ? time_percentage_completed : map_percentage_completed).toFixed(2) %>%</div>
            <div class="count">
                <% if (yearlyType == 'xp') { %>
                <%= xp.toLocaleString() %> / <%= xp_total.toLocaleString() %> cxp
                <% } else { %>
                <%= count_completed.toLocaleString() %> / <%= count_total.toLocaleString() %> maps
                <% } %>
            </div>
        </div>
        <div class="bar">
            <div class="filling"
                style="width: <%= (yearlyType == 'xp' ? time_percentage_completed : map_percentage_completed) %>%; background: <%= color %>">
            </div>
        </div>
    </div>
</div>