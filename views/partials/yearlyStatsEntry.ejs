<div class="yearlyStatsEntry">
    <div class="year"><%= year %></div>
    <div class="content">
        <div class="values">
            <div class="percent"><%= percentage_completed.toFixed(2) %>%</div>
            <div class="count"><%= count_completed.toLocaleString() %> /
                <%= count_total.toLocaleString() %></div>
        </div>
        <div class="bar">
            <div class="filling" style="width: <%= percentage_completed %>%; background: <%= color %>">
            </div>
        </div>
    </div>
</div>